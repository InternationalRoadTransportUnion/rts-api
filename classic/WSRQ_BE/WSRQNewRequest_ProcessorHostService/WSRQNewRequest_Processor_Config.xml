<Config>
  <Section name="PluginManager">
    <Plugin name="DBHelperFactory" objectType="IRU.RTS.CommonComponents.DBHelperFactory, CommonComponents" isRunnable="false" />
    <Plugin name="WSRQNewRequest_Processor" objectType="IRU.RTS.WSRQ.WSRQNewRequest_Processor, WSRQNewRequest_Processor" isRunnable="false" />
    <Plugin name="FileSystemListener" objectType="IRU.RTS.CommonComponents.FileSystemListener, FileListener" isRunnable="true" />
  </Section>
  <Section name="WSRQNewRequest_Processor">
    <WSRQNewRequest_ProcessorSettings DBHelperFactoryPlugin="DBHelperFactory" SchemaFilesPath="Schemas">
    </WSRQNewRequest_ProcessorSettings>
  </Section>
  <Section name="FileSystemListener">
    <FileSystemListener DeliverToPlugin="WSRQNewRequest_Processor" PollFolderLocation="WSRQNR_Folders\WSRQNRInDrop" WorkingFolderLocation="WSRQNR_Folders\WSRQNRWorkingFolder" ArchiveFolderLocation="WSRQNR_Folders\WSRQNRUploadFileArchive" MaxConcurrentProcessCount="2" ErrorFolderLocation="WSRQNR_Folders\ErrorProcessingFiles" FilePattern="*.req" ForceScanInterval="3">
    </FileSystemListener>
  </Section>
  <Section name="DBHelperFactory">
    <ConnectionStrings>
      <DB Name="WSRQDB">
        <!-- Base64 encoded -->
        <ConnectionString>SERVER=172.25.13.50;DATABASE=WSRQ_DB;Integrated Security=SSPI;connect timeout =200</ConnectionString>
      </DB>
    </ConnectionStrings>
  </Section>
</Config>
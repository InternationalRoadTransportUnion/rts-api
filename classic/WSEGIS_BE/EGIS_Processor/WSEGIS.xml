<Config>
    <Section name="PluginManager">
        <Plugin name="DBHelperFactory" objectType="IRU.RTS.CommonComponents.DBHelperFactory, CommonComponents" isRunnable="false" />
        <Plugin name="EGIS_RemotingHelper" objectType="IRU.RTS.WSEGIS.EGIS_RemotingHelper, EGIS_Processor" isRunnable="true" />
        <Plugin name="InMemoryCache" objectType="IRU.RTS.CommonComponents.FileCache, CommonComponents" isRunnable="false" />
    </Section>
    <Section name="EGIS_RemotingHelper">
        <EGIS_RemotingHelperSettings DBHelperFactoryPlugin="DBHelperFactory" EGISDBName="EGISDB" SubscriberDBName="SubscriberDB" EncryptionKeysTable="EncryptionKeys" InMemoryCachePlugin="InMemoryCache" RemotingPort="9008" CryptoProviderEndpoint="tcp://127.0.0.1:9012/CryptoProvider.rem" RemotingEndPoint="WSEGIS_Processor.rem" SchemaFilesPath="Schemas" CarnetingServiceEndpoint="http://172.25.12.50:9999/StandardCarnetingService">
        </EGIS_RemotingHelperSettings>
    </Section>
    <Section name="InMemoryCache">
        <InMemoryFileCacheSettings ScanDirectory="CachedFiles" FilePattern="*.xml">
        </InMemoryFileCacheSettings>
    </Section>
    <Section name="DBHelperFactory">
        <ConnectionStrings>
            <DB Name="EGISDB">
                <!-- Base64 encoded -->
                <ConnectionString>SERVER=172.25.13.50;DATABASE=RTS_LOG;Integrated Security=SSPI;connect timeout =200</ConnectionString>
                <SchemaName>EGIS</SchemaName>
            </DB>
            <DB Name="SubscriberDB">
                <!-- Base64 encoded -->
                <ConnectionString>SERVER=172.25.13.50;DATABASE=WS_Subscriber_db;Integrated Security=SSPI;connect timeout =200</ConnectionString>
            </DB>
        </ConnectionStrings>
    </Section>
</Config>
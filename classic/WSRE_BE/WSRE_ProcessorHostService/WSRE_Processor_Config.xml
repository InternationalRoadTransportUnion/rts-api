<Config>
  <Section name="PluginManager">
    <Plugin name="DBHelperFactory" objectType="IRU.RTS.CommonComponents.DBHelperFactory, CommonComponents" isRunnable="false" />
    <Plugin name="WSRE_Processor" objectType="IRU.RTS.WSRE.WSRE_Processor, WSRE_Processor" isRunnable="false" />
    <Plugin name="FileSystemListener" objectType="IRU.RTS.CommonComponents.FileSystemListener, FileListener" isRunnable="true" />
    <!--<Plugin name="InMemoryCache" objectType="IRU.RTS.CommonComponents.FileCache, CommonComponents"
			isRunnable="false" />-->
  </Section>
  <Section name="WSRE_Processor">
    <WSRE_ProcessorSettings DBHelperFactoryPlugin="DBHelperFactory" InMemoryCachePlugin="InMemoryCache" DispatchFileDropPath="WSRE_Folders/DispatchPath" TemporaryFolderPath="WSRE_Folders/Temp_Dispatch" CryptoProviderEndpoint="tcp://localhost:9012/CryptoProvider.rem" SchemaFilesPath="Schemas">
    </WSRE_ProcessorSettings>
  </Section>
  <!--</Section>-->
  <Section name="FileSystemListener">
    <FileSystemListener DeliverToPlugin="WSRE_Processor" PollFolderLocation="./../WSST_Ex/WSREFR_Folders/DoubleAgentDropPath" WorkingFolderLocation="WSRE_Folders/ReplyWorkingFolder" ArchiveFolderLocation="WSRE_Folders/ReplyUploadFileArchive" MaxConcurrentProcessCount="2" ErrorFolderLocation="WSST_Folders/ErrorProcessingFiles" FilePattern="*.Reply" ForceScanInterval="3">
    </FileSystemListener>
  </Section>
  <Section name="DBHelperFactory">
    <ConnectionStrings>
      <DB Name="WSRE_Internal_DB">
        <!-- Base64 encoded -->
        <ConnectionString>SERVER=172.25.13.50;DATABASE=WSST_INTERNAL_DB;Integrated Security=SSPI;connect timeout =200</ConnectionString>
      </DB>
      <DB Name="SubscriberDB">
        <ConnectionString>SERVER=172.25.13.50;DATABASE=WS_SUBSCRIBER_DB;Integrated Security=SSPI;connect timeout =200</ConnectionString>
      </DB>
    </ConnectionStrings>
  </Section>
</Config>

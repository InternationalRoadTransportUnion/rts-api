<?xml version="1.0" encoding="UTF-8"?>
<assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
	<id>singletons-symbols</id>
	<formats>
		<format>zip</format>
	</formats>
	
	<includeBaseDirectory>false</includeBaseDirectory>

	<fileSets>
		<fileSet>
			<directory>target/singletons/RTS</directory>
			<outputDirectory>/</outputDirectory>
			<includes>
				<include>**/*.pdb</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>target/services/RTS/WSST_In</directory>
			<outputDirectory>/WSST_In</outputDirectory>
			<includes>
				<include>**/*.pdb</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>target/services/RTS/WSRE_In</directory>
			<outputDirectory>/WSRE_In</outputDirectory>
			<includes>
				<include>**/*.pdb</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>target/services/RTS/CryptoHost</directory>
			<outputDirectory>/CryptoHost</outputDirectory>
			<includes>
				<include>**/*.pdb</include>
			</includes>
		</fileSet>
	</fileSets>

</assembly>



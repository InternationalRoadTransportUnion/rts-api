<?xml version="1.0" encoding="utf-8" ?>
<configuration>
<system.diagnostics>
       <switches>
          <add name="General" value="4" />
       </switches>
       <trace autoflush="true" indentsize="4">
          <listeners>
          <!-- change the log file name if u run two instances of same testhost as it will cause error -->
             <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="TestHostLog.log" />
          </listeners>
       </trace>
    </system.diagnostics>
</configuration>

<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://www.iru.org/model/tir-carnet-1" elementFormDefault="qualified" targetNamespace="http://www.iru.org/model/tir-carnet-1" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="TIRCarnetNumberType">
    <xs:restriction base="xs:token">
      <xs:pattern value="[A-Z]{2}[0-9]{8}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="VoletPageNumberType">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="20" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
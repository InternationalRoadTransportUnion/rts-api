			</DB>
			<DB Name="SubscriberDB">
			<ConnectionString>SERVER=DOCTOR43\I0;DATABASE=WS_SUBSCRIBER_DB;uid=rtsuser;pwd=rtsuser;connect timeout =20</ConnectionString>
